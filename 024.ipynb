    "import math\n",
    "total = 999999\n",
    "s = list(range(10))\n",
    "ans = \"\"\n",
    "\n",
    "for i in range(10):\n",
    "    diff = math.factorial(9 - i)\n",
    "    \n",
    "    idx = 0\n",
    "    while total - diff >= 0:\n",
    "        total = total - diff\n",
    "        idx += 1\n",
    "    \n",
    "    ans += str( s.pop(idx) )\n",
    "    \n",
    "print(ans)"
   ]
